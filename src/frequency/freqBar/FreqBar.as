package frequency.freqBar{    import flash.display.MovieClip;    //import org.casalib.display.CasaMovieClip;    import util.ColorUtil;    //import org.casalib.display.CasaShape;    import org.casalib.util.DrawUtil;    import flash.geom.Point        public class FreqBar extends MovieClip    {       //public var mcFill:CasaMovieClip;        public var id:String;        public function FreqBar(inObj:Object)        {            super();            id = inObj.Id;                        x=inObj.X;            y = inObj.Y;                             this.graphics.beginFill(inObj.Color,inObj.Alpha);            DrawUtil.drawShape(this.graphics, new Array(new Point(0, 0), new Point(0, inObj.Height), new Point(inObj.Width, inObj.Height), new Point(inObj.Width, 0)));            this.graphics.endFill();            this.graphics.lineStyle(0, inObj.Color);            DrawUtil.drawPath(this.graphics, new Array(new Point(0, 0), new Point(0, inObj.Height), new Point(inObj.Width, inObj.Height), new Point(inObj.Width, 0), new Point(0, 0)));            //DrawUtil.drawPath(this.graphics, new Array(new Point(0, 0), new Point(0, inObj.Height)));            this.graphics.endFill();                    }            }}